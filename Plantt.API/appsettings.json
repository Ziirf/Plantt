{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithExceptionDetails", "WithEnvironment" ]
  },
  "ConnectionStrings": {
    "Plantt": "connectionString Here"
  },
  "JWTSettings": {
    "SecretKey": "InsertALongAndSecureKeyHereThatYouDoNotShareWithAnyone",
    "Issuer": "issuer.com",
    "MinutesToLive": 15
  },
  "RefreshTokenSettings": {
    "RefreshTokenLength": 32,
    "RefreshFamilyLength": 24,
    "DaysToLive": 300
  },
  "PasswordSettings": {
    "SaltSize": 32,
    "PasswordHashSize": 64,
    "MinIterations": 10000,
    "MaxIterations": 50000
  },
  "AllowedHosts": "*"
}